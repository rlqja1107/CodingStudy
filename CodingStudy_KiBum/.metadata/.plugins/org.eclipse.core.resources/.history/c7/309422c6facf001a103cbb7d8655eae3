package DynamicProgramming;

import java.util.ArrayList;

public class GoSchool {
	private static int[] makeArray(int i,int j) {
		int temp[]= {i,j};
		return temp;
	}
	private static boolean check(int []arr,ArrayList<int []> array) {
		if(array.contains(arr))return true;
		return false;
	}
	
	public static int solution(int m, int n, int[][]puddles) {
		ArrayList<int []> puddle=new ArrayList<int []>();
		for(int i[]:puddles) {
			puddle.add(i);
		}
		
		int sum[][]=new int[m+1][n+1];
		for(int i=1;i<=sum.length;i++) {
			for(int j=1;j<=sum.length;j++) {
				int temp[]= {i,j};
				if(i==1||j==1) 
					if(!check(temp,puddle)) sum[i][j]=1;						
				
				else {
					if(!check(temp,puddle)) {
						sum[i][j]=sum[i-1][j]+sum[i][j-1];
					}
					else sum[i][j]=0;
				}
				
			}
		}
		return sum[m+1][n+1];
		
	}
	public static void main(String args[]) {
		int a[][]= {{2,2}};
		System.out.println(solution(4,3,a));
	}
	
}
