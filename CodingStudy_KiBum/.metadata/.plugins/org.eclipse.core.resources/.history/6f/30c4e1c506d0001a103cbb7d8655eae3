package DynamicProgramming;

import java.util.ArrayList;

public class GoSchool{
	public static int solution(int m, int n, int[][]puddles) { 	
		int sum[][]=new int[n][m];
		int flag=0;
		for(int i=0;i<puddles.length;i++) {
			int first=puddles[i][0];
			int second=puddles[i][1];
			sum[second-1][first-1]=-1;
		}
		System.out.println("Checking");
		for(int i=0;i<n;i++) {
			for(int j=0;j<m;j++) {
				if(i==0) {
					if(sum[i][j]==-1) {sum[i][j]=0; break;}
					else sum[i][j]=1;
				}
				if(j==0&&flag==0) {
					if(sum[i][j]==-1) {
						sum[i][j]=0;
						flag++;
					}
					else sum[i][j]=1;
				}
				else{
				if(sum[i][j]!=-1) {
					sum[i][j]=sum[i-1][j]+sum[i][j-1];
				}
				else { sum[i][j]=0;
				}
				}
			}
		}
		for(int i=0;i<n;i++) {
			for(int j=0;j<m;j++) {
				System.out.print(sum[i][j]+" ");
			}
			System.out.println();
		}
		return sum[n-1][m-1]%1000000007;

	}
	public static void main(String args[]) {
		int a[][]= {{1,3},{2,1},{3,3}};
		System.out.println(solution(4,3,a));
	}


}
