package DynamicProgramming;

import java.util.ArrayList;

public class GoSchool{
	private static boolean check(int []arr,int [][]array) {
		int first=arr[0];
		int second=arr[1];
		for(int i=0;i<array.length;i++) {
			if(array[i][0]==first&&array[i][1]==second)return true;
			
		}
		return false;
	}
	public static long solution(int m, int n, int[][]puddles) {	
		long sum[][]=new long[n+1][m+1];
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=m;j++) {
				int temp[]= {i,j};
				if(i==1||j==1) {
					if(!check(temp,puddles)) sum[i][j]=1;						
				}
				else {
					if(!check(temp,puddles)) {
						sum[i][j]=sum[i-1][j]+sum[i][j-1];
					}
					else { sum[i][j]=0;
					System.out.println("Checking");
					
					}
				}
				
			}
		}
		return sum[n][m]%1000000007;
		
	}
	public static void main(String args[]) {
		int a[][]= {{2,2}};
		System.out.println(solution(4,3,a));
	}
	
	
}
