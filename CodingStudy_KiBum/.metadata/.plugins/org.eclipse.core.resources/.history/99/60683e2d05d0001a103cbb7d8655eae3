package DynamicProgramming;

import java.util.ArrayList;

public class GoSchool{
	private static void makeZero(int start,int [][]arr) {
		
		for(int i=start;i<arr.length;i++) {

			arr[i][1]=0;
		}
	}
	public static int solution(int m, int n, int[][]puddles) { 	
		int sum[][]=new int[n+1][m+1];
		int flag=0;
		for(int i=0;i<puddles.length;i++) {
			int first=puddles[i][0];
			int second=puddles[i][1];
			sum[first][second]=-1;
		}
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=m;j++) {
				if(i==1) {
					if(sum[i][j]==-1) {sum[i][j]=0; break;}
					else sum[i][j]=1;
				}
				if(j==1&&flag==0) {
					if(sum[i][j]==-1) {
						makeZero(i,sum);
						flag++;
					}
					else sum[i][j]=1;
				}
				else{
				if(sum[i][j]!=-1) {
					sum[i][j]=sum[i-1][j]+sum[i][j-1];
				}
				else { sum[i][j]=0;
				}
				}
			}
		}
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=m;j++) {
				System.out.print(sum[i][j]+" ");
			}
			System.out.println();
		}
		return sum[n][m]%1000000007;

	}
	public static void main(String args[]) {
		int a[][]= {{1,3},{2,4},{3,2}};
		System.out.println(solution(5,4,a));
	}


}
